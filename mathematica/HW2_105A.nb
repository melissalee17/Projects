    RowBox[{"(", 
     RowBox[{
      RowBox[{"3", " ", "mg", " ", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"l", " ", 
          RowBox[{"cos", "[", 
           RowBox[{
            SubscriptBox["\[Theta]", "1"], "[", "t", "]"}], "]"}]}], "+", 
         RowBox[{"l", " ", 
          RowBox[{"cos", "[", 
           RowBox[{
            SubscriptBox["\[Theta]", "2"], "[", "t", "]"}], "]"}]}], "+", 
         RowBox[{"lcos", "[", 
          RowBox[{
           SubscriptBox["\[Theta]", "1"], "[", "t", "]"}], "]"}]}], ")"}]}], 
      "+", 
      RowBox[{"m", " ", 
       RowBox[{"(", 
        RowBox[{
         SuperscriptBox[
          RowBox[{
           SuperscriptBox["lcos", "\[Prime]",

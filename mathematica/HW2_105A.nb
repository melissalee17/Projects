            MultilineFunction->None], "[", 
           RowBox[{
            SubscriptBox["\[Theta]", "1"], "[", "t", "]"}], "]"}], "2"]}], 
        ")"}], " ", 
       SuperscriptBox[
        RowBox[{
         SuperscriptBox[
          SubscriptBox["\[Theta]", "1"], "\[Prime]",
          MultilineFunction->None], "[", "t", "]"}], "2"]}], "+", 
      RowBox[{
       SuperscriptBox["l", "2"], " ", "m", " ", 
       RowBox[{"(", 
        RowBox[{
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{
            RowBox[{
             RowBox[{
              SuperscriptBox["cos", "\[Prime]",
               MultilineFunction->None], "[", 
              RowBox[{
               SubscriptBox["\[Theta]", "1"], "[", "t", "]"}], "]"}], " ", 
             RowBox[{
              SuperscriptBox[
               SubscriptBox["\[Theta]", "1"], "\[Prime]",
               MultilineFunction->None], "[", "t", "]"}]}], "+", 
            RowBox[{
             RowBox[{
              SuperscriptBox["cos", "\[Prime]",
               MultilineFunction->None], "[", 
              RowBox[{
               SubscriptBox["\[Theta]", "2"], "[", "t", "]"}], "]"}], " ", 
             RowBox[{
              SuperscriptBox[
               SubscriptBox["\[Theta]", "2"], "\[Prime]",
               MultilineFunction->None], "[", "t", "]"}]}]}], ")"}], "2"], 
         "+", 
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{
            RowBox[{
             RowBox[{
              SuperscriptBox["sin", "\[Prime]",
               MultilineFunction->None], "[", 
              RowBox[{
               SubscriptBox["\[Theta]", "1"], "[", "t", "]"}], "]"}], " ", 
             RowBox[{
              SuperscriptBox[
               SubscriptBox["\[Theta]", "1"], "\[Prime]",
               MultilineFunction->None], "[", "t", "]"}]}], "+", 
            RowBox[{
             RowBox[{
              SuperscriptBox["sin", "\[Prime]",
               MultilineFunction->None], "[", 
              RowBox[{
               SubscriptBox["\[Theta]", "2"], "[", "t", "]"}], "]"}], " ", 
             RowBox[{
              SuperscriptBox[
               SubscriptBox["\[Theta]", "2"], "\[Prime]",
               MultilineFunction->None], "[", "t", "]"}]}]}], ")"}], "2"]}], 
        ")"}]}]}], ")"}]}]}],
  SequenceForm[
  "Therefore, the Lagrangian is equal to ", 
   Rational[2, 9] (3 $CellContext`mg ($CellContext`l $CellContext`cos[
         Subscript[$CellContext`\[Theta], 
          1][$CellContext`t]] + $CellContext`l $CellContext`cos[
         Subscript[$CellContext`\[Theta], 
